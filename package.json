{
  "name": "lua-js",
  "version": "1.0.1",
  "description": "The Lua binding for JavaScript.",
  "main": "main.js",
  "scripts": {
    "install": "cd vendor && curl -R -O http://www.lua.org/ftp/lua-5.4.3.tar.gz && tar zxf lua-5.4.3.tar.gz && cd lua-5.4.3 && MYCFLAGS=-fPIC make all -j4 && npm run rebuild",
    "rebuild": "node-gyp rebuild",
    "dev": "node-gyp build --debug",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/approvers/lua-js.git"
  },
  "keywords": [
    "Lua",
    "Node",
    "JavaScript"
  ],
  "author": "MikuroXina",
  "license": "Apache-2.0",
  "gypfile": true,
  "bugs": {
    "url": "https://github.com/approvers/lua-js/issues"
  },
  "homepage": "https://github.com/approvers/lua-js#readme",
  "dependencies": {
    "bindings": "^1.5.0",
    "nan": "^2.14.2"
  },
  "devDependencies": {
    "jest": "^26.6.3"
  }
}
